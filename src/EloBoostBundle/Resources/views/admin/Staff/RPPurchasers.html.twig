                        <li>
                            <a href="#">
                                <div>
                                    <i class="fa {% if notification.type=='New Transaction' %}
                                    {{ "fa-shopping-cart" }}
                                    {% elseif notification.type=='New Boost' %}
                                    {{ "fa-arrow-up" }}
                                    {% elseif notification.type=='New RP Purchase' %}
                                    {{ "fa-money" }}
                                    {% else %}
                                    {{ "fa-envelope" }}
                                    {% endif %} fa-fw"></i>
                                    {{ notification.type }}
                                    <span class="pull-right text-muted small">
                                            {% if (( "now"|date("d")- notification.createdat|date("d")==0) and (("now"|date("H")) == (notification.createdat|date("H"))) ) %}
                                                {{ "now"|date('i')-notification.createdat|date('i') }} minutes ago
                                            {% elseif  ((("now"|date("d")- notification.createdat|date("d")==0)) and ("now"|date("H")- notification.createdat|date("H")==1) and ("now"|date("i") < notification.createdat|date("i"))) %}
                                                {{ 60-(notification.createdat|date('i')-"now"|date('i')) }} minutes ago
                                            {% elseif ( "now"|date("d")- notification.createdat|date("d")==0) %}
                                                {{ "now"|date('H')-notification.createdat|date('H') }} hours ago
                                            {% elseif ( "now"|date("m") == notification.createdat|date("m")) and ("now"|date("d") - notification.createdat|date("d"))==1%}
                                                {{ "Yesterday" }}
                                            {% else %}
                                                {{ notification.createdat|date }}
