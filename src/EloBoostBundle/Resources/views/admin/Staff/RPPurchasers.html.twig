            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        All Boosts
                    </div>
                    <!-- /.panel-heading -->
                    <div class="panel-body">
                        <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
                            <thead>
                            <tr>
                                <th>ID</th>
                                <th>From</th>
                                <th>To</th>
                                <th>Status</th>
                                <th>Booster</th>
                                <th>Paid</th>
                                <th>UserID</th>
                                <th>Code</th>
                                <th>Created At</th>
                                <th>Finished At</th>
                            </tr>
                            </thead>
                            <tbody>
                            {% for boost in boosts %}
                                <tr>
                                    <td>{{ boost.id }}</td>
                                    <td>{{ boost.fromLG }}
                                        {% if  boost.fromDiv == 'Division V'%}
                                            V
                                        {% elseif boost.fromDiv == 'Division IV'%}
                                            IV
                                        {% elseif boost.fromDiv == 'Division IIi'%}
                                            III
                                        {% elseif boost.fromDiv == 'Division II'%}
                                            II
                                        {% else %}
                                            I
                                        {% endif %} , {{ boost.fromLP }}
                                    </td>
                                    <td>{{ boost.toLG }}
                                        {% if  boost.toDiv == 'Division V'%}
                                            V
                                        {% elseif boost.toDiv == 'Division IV'%}
                                            IV
                                        {% elseif boost.toDiv == 'Division IIi'%}
                                            III
                                        {% elseif boost.toDiv == 'Division II'%}
                                            II
                                        {% else %}
                                            I
                                        {% endif %}
                                    </td>
                                    <td>{{ boost.status }}</td>
                                    <td>
                                        {% if boost.booster == null %}
                                            <strong style="color:red;">Unassigned</strong>
                                        {% else %}
                                            {{ boost.booster }}
                                        {% endif %}
                                    </td>
                                    <td>{{ boost.paid }}</td>
                                    <td>{{ boost.userId.id }}</td>
                                    <td>{{ boost.code }}</td>
                                    <td>{{ boost.createdat |date('d/m/y') }}</td>
                                    <td>
                                        {% if boost.finishedAt == null %}
                                            <strong style="color:red;">Not Finished</strong>
                                        {% else %}
                                            {{ boost.finishedAt |date('d/m/y') }}
                                        {% endif %}
                                    </td>
                                </tr>
                            {% endfor %}
                            </tbody>
